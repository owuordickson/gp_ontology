@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp> .

<http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#composedOf
gp:composedOf rdf:type owl:ObjectProperty ;
              owl:inverseOf gp:isPartOf .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#hasItem
gp:hasItem rdf:type owl:ObjectProperty ;
           owl:inverseOf gp:isItemOf ;
           rdf:type owl:InverseFunctionalProperty ;
           rdfs:domain gp:Attribute ;
           rdfs:range gp:Gradual_Item .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#hasVariation
gp:hasVariation rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#isItemOf
gp:isItemOf rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain gp:Gradual_Item ;
            rdfs:range gp:Attribute ;
            rdfs:comment "Functional property: prevents a GI instance to be an item of 2 (or more) attributes" .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#isPartOf
gp:isPartOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#isRelatedTo
gp:isRelatedTo rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#hasSupport
gp:hasSupport rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#hasSupportGI
gp:hasSupportGI rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf gp:hasSupport ;
                rdfs:domain gp:Gradual_Item ;
                rdfs:range xsd:float ;
                owl:propertyDisjointWith gp:hasSupportGP .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#hasSupportGP
gp:hasSupportGP rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf gp:hasSupport ;
                rdfs:domain gp:Gradual_Pattern ;
                rdfs:range xsd:float .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Attribute
gp:Attribute rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty gp:hasItem ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass gp:Attribute-
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty gp:hasItem ;
                               owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass <http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Attribute+>
                             ] ;
             owl:disjointWith gp:Gradual_Pattern ,
                              gp:Variation ;
             rdfs:comment "It can only be used to create exactly 1 Gradual Item with - variation and exactly 1 Gradual Item with + variation." ,
                          "Retrieved from a data set (i.e., column or feature). It holds a set of objects (or rows)." .

[ rdf:type owl:Axiom ;
   owl:annotatedSource gp:Attribute ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty gp:hasItem ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass gp:Attribute-
                       ] ;
   rdfs:comment "Cardinality: restricts only 1 GI- to be the item of the attribute."
 ] .

[ rdf:type owl:Axiom ;
   owl:annotatedSource gp:Attribute ;
   owl:annotatedProperty rdfs:subClassOf ;
   owl:annotatedTarget [ rdf:type owl:Restriction ;
                         owl:onProperty gp:hasItem ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass <http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Attribute+>
                       ] ;
   rdfs:comment "Cardinality: restricts only 1 GI- to be the item of the attribute."
 ] .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Attribute-
gp:Attribute- rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( gp:Gradual_Item
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty gp:hasVariation ;
                                                           owl:someValuesFrom <http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#->
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:comment "Equivalent to a Gradual Item and has Variation -" .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Gradual_Item
gp:Gradual_Item rdf:type owl:Class ;
                rdfs:subClassOf [ owl:intersectionOf ( gp:Attribute
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty gp:hasVariation ;
                                                         owl:someValuesFrom gp:Variation
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
                rdfs:comment "GI is a subclass of (Attribute and has a Variation - or +)" .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Gradual_Pattern
gp:Gradual_Pattern rdf:type owl:Class ;
                   owl:disjointWith gp:Variation .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Variation
gp:Variation rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:unionOf ( <http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#+>
                                                 <http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#->
                                               )
                                 ] ;
             rdfs:comment """either + or - (i.e., decreasing or increasing).

This is a property of a Gradual Item.""" .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#+
<http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#+> rdf:type owl:Class ;
                                                             rdfs:subClassOf gp:Variation ;
                                                             rdfs:comment "Implies increasing (used to describe a Gradual Item whose variation is increasing)" .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#-
<http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#-> rdf:type owl:Class ;
                                                             rdfs:subClassOf gp:Variation ;
                                                             rdfs:comment "Implies decreasing (used to describe a Gradual Item whose variation is decreasing)" .


###  http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Attribute+
<http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#Attribute+> rdf:type owl:Class ;
                                                                      owl:equivalentClass [ owl:intersectionOf ( gp:Gradual_Item
                                                                                                                 [ rdf:type owl:Restriction ;
                                                                                                                   owl:onProperty gp:hasVariation ;
                                                                                                                   owl:someValuesFrom <http://www.semanticweb.org/owuorjnr/ontologies/2021/9/gp#+>
                                                                                                                 ]
                                                                                                               ) ;
                                                                                            rdf:type owl:Class
                                                                                          ] ;
                                                                      rdfs:comment "Equivalent to a Gradual Item and has Variation +" .


